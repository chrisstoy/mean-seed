{
  "compilerOptions": {
    "baseUrl": "./app",
    "compileOnSave": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ES5"
  }
}
